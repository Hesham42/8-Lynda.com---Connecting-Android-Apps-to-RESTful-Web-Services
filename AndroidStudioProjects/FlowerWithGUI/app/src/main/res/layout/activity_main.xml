<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.guinness.flowerwithgui.MainActivity">

    <Button
        android:id="@+id/XMLPARSING"
        android:text="@string/Xmlparsing"
        android:layout_width="368dp"
        android:layout_height="0dp"
        android:onClick="XMLparssing"
        tools:layout_editor_absoluteY="405dp"
        tools:layout_editor_absoluteX="8dp" />
    <Button
        android:id="@+id/JasonParssing"
        android:text="@string/JASONPARSING"
        android:layout_width="360dp"
        android:layout_height="61dp"
        android:onClick="Jasonparssing"
        tools:layout_editor_absoluteY="35dp"
        tools:layout_editor_absoluteX="8dp" />
    <Button
        android:onClick="jasonParssingSecure"
        android:id="@+id/JasonParssingSecure"
        android:text="@string/JASONParsingSecure"
        android:layout_width="358dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="200dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintRight_toRightOf="parent" />


</android.support.constraint.ConstraintLayout>

